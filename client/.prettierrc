{
  "printWidth": 80,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/store/(.*)$",
    "^@/hooks/(.*)$",
    "^@/services/(.*)$",
    "^@/screens/(.*)$",
    "^@/components/(.*)$",
    "^@/ui/(.*)$",
    "^@/shared/(.*)$",
    "^@/utils/(.*)$",
    "^@/config/(.*)$",
    "^@/assets/(.*)$",
    "^@../(.*)",
    "^@./(.*)",
    "^@(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}